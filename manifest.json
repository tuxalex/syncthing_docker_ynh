{
    "name": "Syncthing_docker",
    "id": "syncthing_docker",
    "packaging_format": 1,
    "description": {
        "en": "Build app inside docker container for yunohost",
        "fr": "Construit l'application à l'intérieur d'un conteneur docker pour yunohost"
    },
    "url": "github.com",
    "license": "free",
    "maintainer": {
        "name": "tuxalex",
        "email": "tuxy@tuxcloud.fr"
    },
    "requirements": {
        "yunohost": "1.1"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Chooze your domain",
                    "fr": "Choisissez votre domaine"
                },
                "example": "domain.org"
            },
            {
                "name": "path",
                "type": "path",
                "ask": {
                    "en": "Choose a path",
                    "fr": "Choisissez un chemin"
                },
                "default": "/test",
                "example": "/path"
            },
            {
                "name": "is_public",
                "ask": {
                    "en": "Is it a public redirection ?",
                    "fr": "Est-ce une redirection publique ?"
                },
                "choices": [
                    "Yes",
                    "No"
                ],
                "default": "No"
            },
	        {
                "name": "username",
                "ask": {
                    "en": "Install for witch user ?",
                    "fr": "Installer pour quel utilisateur ?"
                },
                "example": "johndoe"
            },
            {
                "name": "language",
                "ask": {
                    "en": "Choose the application language",
                    "fr": "Choisissez la langue de l'application"
                },
                "choices": ["fr", "en"],
                "default": "fr"
            }
        ]
    }
}
